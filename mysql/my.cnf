[mysqld]
port            = 3306
datadir         = /var/lib/mysql
socket          = /var/lib/mysql/mysql.sock
tmpdir		    = /tmp
user            = mysql
symbolic-links  = 0
wait_timeout    = 60
interactive_timeout = 60

[client]
default-character-set = utf8

[mysqldump]
default-character-set = utf8

[mysql]
default-character-set = utf8